{
    "name": "Chat gpt",
    "description": "Chat GPT'ni chrome extansion varianti, Daturchilar  va o'zim uchun qulaylik yaratish uchun yasadim",
    "version": "0.1.0",
    "manifest_version": 3,
    "icons": {
        "16":"/static/images/logo16.png",
        "32":"/static/images/logo32.png",
        "48":"/static/images/logo48.png",
        "128":"/static/images/logo128.png"
    },
    "action": {
        "default_popup": "index.html",
        "default_icon": {
            "16":"/static/images/logo16.png",
            "32":"/static/images/logo32.png",
            "48":"/static/images/logo48.png",
            "128":"/static/images/logo128.png"
        }
    },
    "options_page": "options.html",
    "permissions": [
        "storage",
        "activeTab",
        "scripting",
        "tabs"
    ],
    "host_permissions": [
        "https://www.google.com/*"
    ]
}